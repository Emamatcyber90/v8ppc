#!/bin/bash
file=${@: -1}
if [[ "${file##*.}" == "map" ]]; then
  for f in /tmp/elfdump*
  do
    objdump $* "$f" 2>&1 | grep -v "File format" | grep -v "Ë†$" 
  done
else
  objdump $*
fi
              
